<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <param name="robot_description" command="cat '$(find robo-arm-gazebo)/models/environment/book.urdf'" />
    
    <node name="book_spawner_client" pkg="robo-arm-gazebo" type="book_spawner" output="screen"
          args="-urdf -param robot_description -model book" />
          
</launch>
